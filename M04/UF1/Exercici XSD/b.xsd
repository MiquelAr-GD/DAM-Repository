      <xs:sequence>
        <xs:element name="soci" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
                <xs:element name="nom" maxOccurs="unbounded"/>
                <xs:element name="cognoms">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element name="c" type="xs:string" maxOccurs="4"/>
